package dowhile;
import java.util.*;

public class sumOfserise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int number =sc.nextInt();
      double count=1;
      double sum=0.0;
      do {
    	  sum=sum+(1/count);
    	  count++;
      }while(count<=number);
      
	}

}
