package pattern;
import java.util.*;s
public class program1 {
    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	int n=sc.nextInt();
    	for(int i=1;i<=n;i++) {
    		for(int j=n;j>1;j--) {
    			System.out.println("* ");
    		}
    		System.out.println();
    	}
    	
    }
}
